<mvc:View
    xmlns:appfinder="sap.ushell.ui.appfinder"
    controllerName="C23C249.invoices.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    displayBlock="true"
>
    <Shell id="_IDGenShell1">
        <App id="_IDGenApp1">
            <pages>
                <Page
                    id="_IDGenPage1"
                    title="{i18n>appTitle}"
                >
                <headerContent>
                					<Button
				icon="sap-icon://action"
				id="_IDGenButton3"
				press=".onShowDialogHeader"
				class="sapUiSmallMarginEnd"
			/>
                
                </headerContent>
                    <content>
                        <mvc:XMLView
                            id="IDGenXMLView1"
                            viewName="C23C249/invoices/view/HelloPanel"
                        />
                              <mvc:XMLView
                            id="IDGenXMLView2"
                            viewName="C23C249/invoices/view/InvoicesList"
                        />
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
